{
  "Routes": [
    {
      "DownstreamPathTemplate": "/Video/{everything}",
      "DownstreamScheme": "http",
      "UseServiceDiscovery": true,
      "ServiceName": "VideoService",
      "UpstreamPathTemplate": "/Video/{everything}",
      "UpstreamHttpMethod": [ "GET" ],
      "Priority": 0
    },
    {
      "DownstreamPathTemplate": "/api/v1/VttFile?filenameVTT={filename}",
      "DownstreamScheme": "http",
      "UseServiceDiscovery": true,
      "ServiceName": "VideoService",
      "UpstreamPathTemplate": "/Video/VttFile/{filename}",
      "UpstreamHttpMethod": [ "GET" ],
      "Priority": 1
    }
  ]
}