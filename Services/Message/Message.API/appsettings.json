{
  "Serilog": {
    "Using": [],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": {
      "Application": "Message.API"
    }
  },
  "spring": {
    "application": {
      "name": "MessageService"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://eureka:8761/eureka/",
      "shouldFetchRegistry": true,
      "shouldRegisterWithEureka": true
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "Endpoint_MessageService": "message-service",
  },
  "Auth0": {
    "Audience": "https://api.familielaiss.de"
  }
}