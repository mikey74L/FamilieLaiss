    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "/var/lib/familielaiss/logs/Mail_API_Log.txt",
          "rollingInterval": "Hour"
        }
      }
    ]
  },
  "AppSettings": {
    "MailtrapAdress": "smtp.mailtrap.io",
    "MailtrapPort": 2525,
    "MailSenderName": "Account - Familie Laiss",
    "MailSenderAdress": "account@familielaiss.de",
    "MailkitAdress": "smtp.strato.de",
    "MailkitPort": 465,
    "MailkitUseSSL": true,
    "MailkitUseSenderAdressAsUser": true,
    "MailType": "Mailtrap",
    "PostgresHost": "postgres_mail",
    "PostgresPort": 5432,
    "PostgresDatabase": "mailservice",
    "PostgresMultiplexing": true
  },
  "Auth0": {
    "Domain": "dev-lr8zm7rl.eu.auth0.com"
  }
