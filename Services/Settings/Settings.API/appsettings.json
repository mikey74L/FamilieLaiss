{
  "Serilog": {
    "Using": [],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": {
      "Application": "Settings.API"
    }
  },
  "spring": {
    "application": {
      "name": "SettingsService"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://eureka:8761/eureka/",
      "shouldFetchRegistry": true,
      "shouldRegisterWithEureka": true
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "PostgresHost": "postgres_settings",
    "PostgresPort": 5432,
    "PostgresDatabase": "settings",
    "PostgresMultiplexing": false,
    "EndpointSettingsService": "settings-service"
  },
  "Auth0": {
    "Audience": "https://api.familielaiss.de"
  }
}