{
  "Serilog": {
    "Using": [],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": {
      "Application": "Upload.API"
    }
  },
  "spring": {
    "application": {
      "name": "UploadService"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://eureka:8761/eureka/",
      "shouldFetchRegistry": true,
      "shouldRegisterWithEureka": true
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "PostgresHost": "postgres_upload",
    "PostgresPort": 5432,
    "PostgresDatabase": "upload",
    "PostgresMultiplexing": false,
    "DirectoryUploadPicture": "/var/lib/familielaiss/Content/Picture",
    "DirectoryUploadPortrait": "/var/lib/familielaiss/Content/Portrait",
    "DirectoryUploadVideo": "/var/lib/familielaiss/Content/Video",
    "TempDirectoryUploadPicture": "/var/lib/familielaiss/Content/TempUploadPicture",
    "TempDirectoryUploadPortrait": "/var/lib/familielaiss/Content/TempUploadPortrait",
    "TempDirectoryUploadVideo": "/var/lib/familielaiss/Content/TempUploadVideo",
    "GoogleMicroserviceUrl": "http://googleservice",
    "GoogleMicroserviceVersion": "v1",
    "EndpointUploadService": "upload-service"
  },
  "Auth0": {
    "Audience": "https://api.familielaiss.de"
  }
}
