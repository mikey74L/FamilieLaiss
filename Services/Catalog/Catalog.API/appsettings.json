{
  "Serilog": {
    "Using": [],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": {
      "Application": "Catalog.API"
    }
  },
  "spring": {
    "application": {
      "name": "CatalogService"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://eureka:8761/eureka/",
      "shouldFetchRegistry": true,
      "shouldRegisterWithEureka": true
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "PostgresHost": "postgres_catalog",
    "PostgresPort": 5432,
    "PostgresDatabase": "catalogservice",
    "PostgresMultiplexing": false,
    "Endpoint_CatalogService": "catalog-service",
    "Endpoint_MessageService": "message-service"
  },
  "Auth0": {
    "Audience": "https://api.familielaiss.de"
  }
}